{
  "drivers": [
    {
      "type": "firefox",
      "options": {
        "arguments": [ "window-size=1600,900" ]
      }
    },
    {
      "type": "chrome",
      "options": {
        "arguments": [ "window-size=1600,900" ]
      }
    },
    {
